{
  "name": "{{name}}",
  "version": "0.2.0",
  "description": "{{description}}",
  "author": "{{author}}",
  "license": "{{license}}",
  "private": false,
  "prepublish": "typed-scss-modules src",
  "scripts": {
    "clean": "rimraf dist && rimraf .next",
    "script": "dotenv -- node ./script/setConfig.js",
    "pre": "rimraf .next && typed-scss-modules src",
    "export": "next export",
    "dev": "next dev -p 3000",
    "prebuild": "rimraf .next",
    "build": "next build",
    "next": "next start -H 0.0.0.0 -p ${PORT:-8080} --keepAliveTimeout 7200000",
    "start": "dotenv -- ts-node --project tsconfig.server.json api/index.ts",
    "start:js": "dotenv -- node dist/index.js",
    "go": "dotenv -e .env.development -- nodemon -e ts,js,json -x ts-node --project tsconfig.server.json --watch api api/index.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "lint": "next lint",
    "test": "jest",
    "analyze": "source-map-explorer .next/static/**/*.js{,.map}",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@as-integrations/fastify": "^2.1.1",
    "@devexpress/dx-react-core": "^4.0.6",
    "@devexpress/dx-react-grid": "^4.0.6",
    "@devexpress/dx-react-grid-material-ui": "^4.0.6",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fastify/compress": "^6.5.0",
    "@fastify/cookie": "^9.2.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.1.0",
    "@fastify/nextjs": "^10.0.1",
    "@hookform/resolvers": "^2.9.11",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "@typegoose/typegoose": "^12.0.0",
    "apexcharts": "^3.45.0",
    "axios": "^0.27.2",
    "class-validator": "^0.14.0",
    "clsx": "^1.2.1",
    "fastify-cron": "^1.3.1",
    "i18next": "21.5.6",
    "lodash.debounce": "^4.0.8",
    "lodash.memoize": "^4.1.2",
    "mailgen": "^2.0.15",
    "next": "^14.1.0",
    "nuudel-core": "^0.3.63",
    "nuudel-main": "^1.1.14",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-i18next": "^14.0.5",
    "sass": "^1.62.1",
    "subscriptions-transport-ws": "^0.11.0",
    "swiper": "6.8.4",
    "swr": "^2.2.4",
    "type-graphql": "2.0.0-beta.3",
    "use-dark-mode": "^2.3.1",
    "xlsx": "^0.17.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.0",
    "@types/crypto-js": "^4.2.1",
    "@types/googlemaps": "^3.43.3",
    "@types/jest": "^24.0.18",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/swiper": "^5.4.3",
    "@types/uuid": "^8.3.1",
    "@types/yup": "^0.29.12",
    "dotenv-cli": "^7.3.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^14.1.0",
    "jest": "^29.7.0",
    "node-sass": "^8.0.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.3.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "typed-scss-modules": "^5.1.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "main": "index.js",
  "repository": "https://github.com/c0des1gn/nuudel.git"
}
