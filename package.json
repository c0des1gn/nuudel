{
  "name": "{{name}}",
  "version": "0.1.5",
  "description": "{{description}}",
  "author": "{{author}}",
  "license": "{{license}}",
  "private": false,
  "prepublish": "tsm src",
  "scripts": {
    "clean": "rimraf dist && rimraf .next",
    "script": "dotenv -e .env.dev -- node ./script/setConfig.js",
    "pre": "rimraf .next && tsm src",
    "export": "next export",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "next": "dotenv -e .env.dev -- next dev -p 3000",
    "build": "rimraf .next && dotenv -e .env.dev -- next build",
    "build:js": "yarn run clean && dotenv -- next build && tsc --p tsconfig.server.json",
    "build:prod": "dotenv -- node ./script/setConfig.js && yarn run pre && next build",
    "start:next": "next start -H 0.0.0.0 -p ${PORT:-8080} --keepAliveTimeout 7200000",
    "start": "dotenv -- ts-node --project tsconfig.server.json api/index.ts",
    "start:js": "dotenv -- node dist/index.js",
    "go": "dotenv -e .env.dev -- nodemon -e ts,js,json -x ts-node --project tsconfig.server.json --watch api api/index.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@devexpress/dx-react-core": "^2.7.5",
    "@devexpress/dx-react-grid": "^2.7.5",
    "@devexpress/dx-react-grid-material-ui": "^2.7.5",
    "@hookform/resolvers": "^2.6.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@typegoose/typegoose": "^8.3.0",
    "apexcharts": "^3.27.1",
    "apollo-server-fastify": "3.6.8",
    "axios": "^0.25.0",
    "class-validator": "^0.13.2",
    "clsx": "^1.1.1",
    "fastify-compress": "^3.6.1",
    "fastify-cookie": "^5.3.2",
    "fastify-cron": "^1.1.2",
    "fastify-jwt": "^3.2.1",
    "fastify-multipart": "^5.1.0",
    "i18next": "^21.2.6",
    "lodash.debounce": "^4.0.8",
    "lodash.memoize": "^4.1.2",
    "mailgen": "^2.0.15",
    "next": "^11.1.4",
    "next-transpile-modules": "^9.0.0",
    "nuudel-core": "^0.1.166",
    "nuudel-main": "^1.0.70",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.28.1",
    "react-i18next": "^11.13.0",
    "sass": "^1.49.0",
    "subscriptions-transport-ws": "^0.11.0",
    "swiper": "6.8.4",
    "swr": "^1.0.1",
    "type-graphql": "^1.1.1",
    "use-dark-mode": "^2.3.1",
    "xlsx": "^0.17.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/googlemaps": "^3.43.3",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.18",
    "@types/lodash.debounce": "^4.0.6",
    "@types/mongodb": "^3.6.20",
    "@types/node": "^12.7.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/swiper": "^5.4.3",
    "@types/uuid": "^8.3.1",
    "@types/yup": "^0.29.12",
    "dotenv-cli": "^3.0.0",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "jest": "^27.3.1",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.14",
    "pino-pretty": "^7.2.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typed-scss-modules": "^4.1.1",
    "typescript": "^4.3.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "main": "index.js",
  "repository": "https://github.com/c0des1gn/nuudel.git"
}
