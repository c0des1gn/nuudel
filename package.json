{
  "name": "\{{name}}",
  "version": "0.1.0",
  "description": "\{{description}}",
  "author": "\{{author}}",
  "license": "\{{license}}",
  "private": false,
  "prepublish": "tsm src",
  "scripts": {
    "clean": "rimraf dist && rimraf .next",
    "pre": "rimraf .next && tsm src",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "rimraf .next && dotenv -e .env.dev -- next build",
    "build:js": "yarn run clean && dotenv -- next build && tsc --project tsconfig.server.json",
    "next:build": "dotenv -- node ./script/setConfig && yarn run pre && dotenv -- next build",
    "build:prod": "dotenv -e .env -- node ./script/setConfig.js && yarn run pre && dotenv -e .env -- next build",
    "next": "dotenv -e .env.dev -- next dev -p 3000",
    "export": "dotenv -- next export",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "go": "dotenv -e .env.dev -- nodemon -e ts,graphql -x ts-node --project tsconfig.server.json api/index.ts --watch",
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node --project tsconfig.server.json api/index.ts",
    "start:js": "dotenv -- nodemon -e js,graphql -x node dist/index.js",
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@devexpress/dx-react-core": "^2.7.5",
    "@devexpress/dx-react-grid": "^2.7.5",
    "@devexpress/dx-react-grid-material-ui": "^2.7.5",
    "@hookform/resolvers": "^2.6.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@typegoose/typegoose": "^8.3.0",
    "apexcharts": "^3.27.1",
    "apollo-server-fastify": "3.4.1",
    "axios": "^0.25.0",
    "class-validator": "^0.13.2",
    "clsx": "^1.1.1",
    "fastify-compress": "^3.6.1",
    "fastify-cookie": "^5.3.2",
    "fastify-cron": "^1.1.2",
    "fastify-jwt": "^3.2.1",
    "fastify-multipart": "^5.1.0",
    "graphql": "^15.7.2",
    "i18next": "^21.2.6",
    "lodash.debounce": "^4.0.8",
    "lodash.memoize": "^4.1.2",
    "mailgen": "^2.0.15",
    "mapbox-gl": "^2.5.1",
    "next": "^11.1.3",
    "next-transpile-modules": "^9.0.0",
    "nuudel-core": "^0.1.30",
    "nuudel-main": "^1.0.8",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-barcode": "^1.4.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.11.1",
    "react-i18next": "^11.13.0",
    "react-loadable": "^5.5.0",
    "sass": "^1.49.0",
    "subscriptions-transport-ws": "^0.11.0",
    "swiper": "6.8.4",
    "swr": "^1.0.1",
    "type-graphql": "^1.1.1",
    "use-dark-mode": "^2.3.1",
    "xlsx": "^0.17.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/googlemaps": "^3.43.3",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.18",
    "@types/lodash.debounce": "^4.0.6",
    "@types/mongodb": "^3.3.10",
    "@types/mongoose": "^5.5.30",
    "@types/node": "^12.7.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/swiper": "^5.4.3",
    "@types/uuid": "^8.3.1",
    "@types/yup": "^0.29.12",
    "dotenv-cli": "^3.0.0",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "jest": "^27.3.1",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.14",
    "pino-pretty": "^7.2.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typed-scss-modules": "^4.1.1",
    "typescript": "^4.3.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "main": "index.js",
  "repository": "https://github.com/c0des1gn/nuudel.git"
}
